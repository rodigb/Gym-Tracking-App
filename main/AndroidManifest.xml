<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.LiftsTracker">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.LiftsTracker.userGuideActivity"></activity>
        <activity android:name="com.example.LiftsTracker.DisplayUserGuideActivity">

        </activity>
        <activity android:name="com.example.LiftsTracker.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.LiftsTracker.MainScreenActivity">

        </activity>


        <activity android:name="com.example.LiftsTracker.StandardsActivity" />
        <activity android:name="com.example.LiftsTracker.RecordActivity">

        </activity>

        <receiver android:name="com.example.LiftsTracker.AlertReciever"/>
    </application>

</manifest>